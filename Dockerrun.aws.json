{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "api1",
      "image": "nbtadmin/sprout-api",
      "hostname": "api",
      "essential": false,
      "memory": 256
    },
    {
      "name": "api2",
      "image": "nbtadmin/sprout-api",
      "hostname": "api",
      "essential": false,
      "memory": 256
    },
    {
      "name": "api3",
      "image": "nbtadmin/sprout-api",
      "hostname": "api",
      "essential": false,
      "memory": 256
    },
    {
      "name": "nginx",
      "image": "nbtadmin/sprout-api-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["api1", "api2", "api3"],
      "memory": 256
    }
  ]
}
